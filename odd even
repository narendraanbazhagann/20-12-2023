

a=[]
n=int(input("Enter the number of elements:"))
for i in range(1,n+1):
    b=int(input("Enter the number"))
    a.append(b)
print(a)
odd=[]
even=[]
for j in a:
    if(j%2==0):
        even.append(j)
    else:
        odd.append(j)
print("odd numbers",odd)
print("Even list:",even)
square=[]
for k in a:
    square.append(k**2)
print("square numbers",square)

